{
  "name": "paw-book-store-mvc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run build:css_no_watch && npm start",
    "start": "node ./app.js",
    "build:css_no_watch": "postcss public/stylesheets/tailwind.css -o public/stylesheets/compiled.css",
    "build:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/compiled.css --watch"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "bcrypt": "4.0.1",
    "body-parser": "1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "eu-vat-rates": "^1.0.1",
    "express": "4.17.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.9.15",
    "morgan": "~1.9.1",
    "node-isbn": "^1.6.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.0.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "prettier": "2.6.2",
    "tailwindcss": "^3.0.24"
  }
}
